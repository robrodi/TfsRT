<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TfsRt.Assets">
    <Canvas Background="Transparent" Height="200" Width="200" x:Key="WorkItemPage">
        <!-- Page -->
        <Path Canvas.Left="5" Canvas.Top="5" Stroke="White" StrokeThickness="8" Data="M 0,0 L 140, 0 L 180,40 L 180, 180 L 0, 180 Z "/>
        <!-- Bent corner-->
        <Path Canvas.Left="145" Canvas.Top="5" StrokeThickness="0" Data="M 0, 00 L 40,40 L 0, 40 Z" Fill="White"/>
    </Canvas>
</ResourceDictionary>
